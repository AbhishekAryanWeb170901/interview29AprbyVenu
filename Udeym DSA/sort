//method 2

function sortNumber(num1, num2) {
    return num1 - num2;
}
console.log([ 2, 5, 9, 6, 20, 6 ].sort(sortNumber))


//sorting alphabet

function arrangeName (name1, name2) {
return name1 - name2;
}
var sortit = ["Abhishek", "Aryan", "Aman", "Nrupul", "Yogesh"].sort(arrangeName)
console.log(sortit);